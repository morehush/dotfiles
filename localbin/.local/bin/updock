#!/bin/bash

dcomp=$HOME/docker/tkhtpc

rsync -turPh /sgt5tb/others/Sync/dockersrv/tkhtpc/docker-compose.yml $dcomp/
rsync -turPh /sgt5tb/others/Sync/dockersrv/tkhtpc/.env $dcomp/

cd $dcomp
docker compose pull
docker compose up -d --remove-orphans
# docker ps --format '{{.Names}}'
docker image prune -af
